<Application x:Class="ContextCreator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ContextCreator"
             xmlns:conv="clr-namespace:ContextCreator.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Converters -->
        <conv:BoolToHighlightConverter x:Key="BoolToHighlightConverter"/>
        <conv:CountToVisibilityConverter x:Key="CountToVisibilityConverter"/>
    </Application.Resources>
</Application>